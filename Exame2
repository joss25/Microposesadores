    MOVFW aux
    MOVWF c8
    
    
    GOTO revision	    ;Chequeo

    BCF PORTC,0		;command mode
    CALL time
    
    MOVLW 0x81		;LCD position
    MOVWF PORTD
    CALL exec
    
    
    GOTO INICIO
 
    
   Lectura
	
	bsf PORTA, 4			
	btfsc PORTA, 3
	call  UNO
	btfsc PORTA, 2		
	call CUATRO
	btfsc PORTA, 1	
	call SIETE
	btfsc PORTA, 0	
	call ASTERISCO
	bcf PORTA, 4
	
	
	bsf PORTA,5 				
	btfsc PORTA, 3
	CALL DOS
	btfsc PORTA,2
	CALL CINCO
	btfsc PORTA, 1		
	CALL OCHO
	btfsc PORTA, 0		
	CALL CERO
	bcf PORTA, 5
	
	
	bsf PORTA, 6
	btfsc PORTA,3
	CALL TRES
	btfsc PORTA,2
	CALL SEIS
	btfsc PORTA,1 
	CALL NUEVE
	btfsc PORTA,0			
	CALL GATO
	bcf PORTA,6
   BTFSS band,0
   GOTO Lectura
   return
 

   	
	
CERO
    bsf band,0 
    MOVLW '0'
    MOVWF aux
    MOVWF PORTD
    CALL exec
    CALL time2
    return 
UNO
    bsf band,0
    MOVLW '1'
    MOVWF aux
    MOVWF PORTD
    CALL exec
    CALL time2
    return 

DOS
    bsf band,0
    MOVLW '2'
    MOVWF PORTD
    MOVWF aux
    CALL exec
    CALL time2
    return 

TRES
    bsf band,0
    MOVLW '3'
    MOVWF PORTD
    MOVWF aux
    CALL exec
    CALL time2
    return 

