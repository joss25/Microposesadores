revision
    BCF band,1
    
    MOVFW p1
    XORWF c1,1
    
    BTFSC c1,0
    bsf band,1
    BTFSC c1,1
    bsf band,1
    BTFSC c1,2
    bsf band,1
    BTFSC c1,3
    bsf band,1
    BTFSC c1,4
    bsf band,1
    BTFSC c1,5
    bsf band,1
    BTFSC c1,6
    bsf band,1
    BTFSC c1,7
    bsf band,1
    
    MOVFW p2
