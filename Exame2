    CALL exec
    CALL time2
    return 
	
ASTERISCO
    bsf band,0
    MOVLW ' '
    MOVWF PORTD
    MOVWF aux
    CALL exec
    CALL time2
    return
    
GATO
   GOTO revision
    return

    
exec
    BSF PORTC,1		;exec
    CALL time
    BCF PORTC,1
    CALL time
    RETURN
	
time
    CLRF i
    MOVLW d'10'
    MOVWF j
ciclo    
    MOVLW d'80'
    MOVWF i
    DECFSZ i
    GOTO $-1
    DECFSZ j
    GOTO ciclo
    RETURN
    
time2
    CLRF i
    MOVLW d'255'
    MOVWF j
ciclo2 
    MOVLW d'255'
    MOVWF i
    DECFSZ i
    GOTO $-1
    DECFSZ j
    GOTO ciclo2
    RETURN
    
